@model IEnumerable<Genre>

<div class="row">
    <div class="col-lg-10">
        <h1>Genre Table</h1>
    </div>
    <div class="col-lg-2">
        <a class="btn btn-primary"asp-controller="Product" asp-action="CreateGenre">Create Genre</a>
    </div>
</div>
<div class="row">
    <div class="col-lg-12">
        <table class="table table-striped">
            <thead>
                <tr>
                    <th >#</th>
                    <th >Name</th>

                </tr>
            </thead>
            <tbody id="GenreTable">
                @await Html.PartialAsync("_GenreTablePartial",Model) 
            </tbody>
        </table>
    </div>

</div>